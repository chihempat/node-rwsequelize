{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "no-console": "off",
        "space-before-function-paren":"off",
        "no-shadow": ["error", { "builtinGlobals": false, "hoist": "functions", "allow": ["error","data","done","err"] }],
        "no-unused-vars":["warn",{"args":"none"}],
        "no-plusplus":"off",
        "indent":["warn",2],
        "eol-last":"off"
    }
}
